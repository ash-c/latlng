{
  "version": "0.0.5",
  "name": "latlng",
  "private": true,
  "description": "Website for adding longitude and latitude data to a csv list of addresses.",
  "author": {
    "name": "Ash Crockett"
  },
  "scripts": {
    "build-test": "cross-env ENV=development webpack --config ./webpack.dev.config.js",
    "build-prod": "cross-env ENV=production webpack -p --config ./webpack.prod.config.js",
    "clean": "npm-run-all --parallel clean:js clean:css",
    "clean:css": "rimraf wwwroot/css/*.css rimraf wwwroot/css/*.map",
    "clean:js": "rimraf wwwroot/js/*.js  rimraf js/*.map",
    "prebuild-test": "npm run clean",
    "prebuild-prod": "npm run clean",
    "prestart": "npm run clean",
    "start": "cross-env ENV=development webpack --watch --progress --colors --config ./webpack.dev.config.js"
  },
  "dependencies": {
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "papaparse": "4.3.6"
  },
  "devDependencies": {
    "@types/googlemaps": "3.30.7",
    "@types/node": "8.0.51",
    "@types/papaparse": "4.1.33",
    "@types/react": "16.0.34",
    "@types/react-dom": "16.0.3",
    "autoprefixer": "7.2.5",
    "axios": "0.17.1",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-loader": "7.1.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "cross-env": "5.1.3",
    "css-loader": "0.28.9",
    "extract-text-webpack-plugin": "3.0.2",
    "friendly-errors-webpack-plugin": "1.6.1",
    "node-sass": "4.7.2",
    "npm-run-all": "4.1.2",
    "postcss-loader": "2.0.10",
    "resolve-url-loader": "2.2.1",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "source-map-loader": "0.2.3",
    "style-loader": "0.20.1",
    "tslint": "5.9.1",
    "tslint-loader": "3.5.3",
    "tslint-react": "3.2.0",
    "ts-loader": "3.3.1",
    "ts-node": "4.1.0",
    "typescript": "2.6.2",
    "uglifyjs-webpack-plugin": "1.1.6",
    "webpack": "3.10.0",
    "webpack-merge": "4.1.1"
  },
  "browserslist": {
    "development": [
      "last 2 version"
    ],
    "production": [
      "last 2 version"
    ]
  }
}
